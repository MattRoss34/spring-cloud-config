{
  "name": "spring-cloud-config",
  "version": "3.0.0",
  "description": "NodeJS application configuration using similar style to Spring Config and using the Spring Cloud Config Server for remote property sources.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "cloud-config-client": "^1.0.0",
    "extend": "^3.0.2",
    "js-yaml": "^3.12.0",
    "logform": "^2.1.2",
    "winston": "^3.2.1",
    "yargs": "^13.3.0"
  },
  "devDependencies": {
    "@types/chai": "4.1.7",
    "@types/mocha": "5.2.7",
    "@types/node": "12.0.10",
    "@types/sinon": "7.0.13",
    "chai": "4.2.0",
    "coveralls": "3.0.4",
    "jsdoc": "3.6.2",
    "mocha": "6.1.4",
    "mocha-lcov-reporter": "1.3.0",
    "nyc": "14.1.1",
    "sinon": "7.3.2",
    "ts-loader": "6.0.4",
    "ts-node": "8.3.0",
    "typescript": "3.5.2"
  },
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf dist",
    "test": "nyc mocha  && nyc report --reporter=text-lcov | coveralls",
    "local-test": "nyc mocha",
    "prepublish": "npm run build",
    "doc": "jsdoc index.js -R README.md -d doc",
    "typecheck": "tsc --noEmit",
    "lint": "tslint --project . --format stylish"
  },
  "nyc": {
    "all": true,
    "check-coverage": true,
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.js",
      "**/models.ts",
      "test"
    ]
  },
  "author": "Matthew Ross",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/mattross34/spring-cloud-config.git"
  },
  "keywords": [
    "Spring Cloud",
    "Cloud Config",
    "Spring Cloud Config",
    "configuration",
    "client",
    "config server"
  ]
}
